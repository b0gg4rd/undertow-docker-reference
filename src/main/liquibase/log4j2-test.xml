<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
  <Appenders>
    <Console name="console" target="SYSTEM_OUT">
      <PatternLayout>
        <Pattern>%d %x %p %c{1} [%t] %m %ex%n</Pattern>
      </PatternLayout>
    </Console>
  </Appenders>
  <Loggers>
    <Root level="${liquibase.logging}">
      <AppenderRef ref="console"/>
    </Root>
  </Loggers>
</Configuration>
